    "$animation_key|default": "alpha_instant_hide",
    "$content_animation_key_suffix|default": "",
    "$chrome_color|default": [0, 0, 0, 1],
    "layer": 57000,
    "variables": [
      {
        "requires": "((not ($title_text - '&fs_fader_state=1&' = $title_text)))",
