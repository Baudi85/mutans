        "$is_visible": true,
        "$animation_key": "alpha_fade_out_2s",
        "$content_animation_key_suffix": "_run"
      },
      {
        "requires": "((not ($title_text - '&fs_fader_state=7&' = $title_text)))",
        "$is_visible": true,
        "$animation_key": "alpha_fade_in_3s"
      },
      {
        "requires": "((not ($title_text - '&fs_fader_state=8&' = $title_text)))",
        "$is_visible": true,
        "$animation_key": "alpha_fade_out_3s",
        "$content_animation_key_suffix": "_run"
      },
      {
        "requires": "((not ($title_text - '&fs_fader_state=9&' = $title_text)))",
